module TestData
{
  struct Msg
  {
    short short_field;
    long long_field;
    unsigned short ushort_field;
    unsigned long ulong_field;
    float float_field;
    double double_field;
    char char_field;
    boolean bool_field;
    octet octet_field;
    string string_field;
    sequence <string> sequence_field;
    short array_field[25];
    float twod_array_field[25][30];
  };
  #pragma keylist Msg short_field long_field string_field

  struct CommonStruct
  {
     string global_name;
     boolean enabled;
  };

  module Inner 
  {
    struct InnerMsg
    {
       long userID;
       string message;
    };

    struct TopicMsg
    {
       InnerMsg inner_msg;
       string topicID;
       ::TestData::CommonStruct common;
       
    };
    #pragma keylist TopicMsg topicID
  };
};
